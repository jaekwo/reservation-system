<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nclab.dao.nclabmapper">

<select id="getEmpno" resultType="com.nclab.vo.combinedvo">
	SELECT
		empno, ename
	FROM
		emp
	WHERE
		empno = '1'		
</select>
</mapper>